  "version": "0.2.0",
  "configurations": [
    {
      "name": "cmd/gdbuild",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/gdbuild",
      "cwd": "${workspaceFolder}",
      "args": [
        "target",
        "-v",
        "--project",
        "example",
        "-p",
        "windows",
        "--release",
        "--dry-run",
        "client"
      ],
      "env": {
        "GDBUILD_HOME": "${workspaceFolder}/.gdbuild"
      }
    }
  ]
}
